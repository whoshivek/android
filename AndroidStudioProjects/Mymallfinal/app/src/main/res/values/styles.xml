<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#FFD835</item>
        <item name="colorPrimaryDark">#000000</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" >
        <item name="android:textColor">#000000</item>
        <item name="iconTint">#000000</item>
    </style>
    <style name="edittext" >
        <item name="colorControlNormal">#c5c5c5</item>
        <item name="colorControlActivated">#FFD835</item>
        <item name="colorControlHighlight">#423F3F</item>
    </style>
    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
    <style name="Divider" tools:ignore="DuplicateDefinition">
        <item name="android:background">?android:attr/listDivider</item>
        <item name="android:backgroundTint">
            @color/colorPrimaryDark
        </item>
    </style>

    <style name="Divider.Horizontal" parent="Divider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item> // You can change thickness here.

    </style>

    <style name="Divider.Vertical" parent="Divider">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">match_parent</item>
    </style>
    <style name="myToolbarTheme">
        <item name="colorControlNormal">#FFFFFF</item>
    </style>
</resources>